	<queryString language="SQL">
		<![CDATA[SELECT
     orden_compras."id_ordencompra" AS orden_compras_id_ordencompra,
     detalle_ordencompras."cantidad" AS detalle_ordencompras_cantidad,
     detalle_ordencompras."precio" AS detalle_ordencompras_precio,
     marcas."descripcion" AS marcas_descripcion,
     orden_compras."fecha" AS orden_compras_fecha,
     orden_compras."id_pedido" AS orden_compras_id_pedido,
     mercaderias."descripcion" AS mercaderias_descripcion
FROM
     "public"."orden_compras" orden_compras INNER JOIN "public"."detalle_ordencompras" detalle_ordencompras ON orden_compras."id_ordencompra" = detalle_ordencompras."id_ordencompra"
     INNER JOIN "public"."mercaderias" mercaderias ON detalle_ordencompras."id_mercaderia" = mercaderias."id_mercaderia"
     INNER JOIN "public"."marcas" marcas ON mercaderias."id_marca" = marcas."id_marca"]]>
	</queryString>
