	<queryString language="SQL">
		<![CDATA[SELECT
     empleados."id_empleados" AS empleados_id_empleados,
     empleados."nombre" AS empleados_nombre,
     empleados."apellido" AS empleados_apellido,
     empleados."direccion" AS empleados_direccion,
     empleados."celular" AS empleados_celular,
     empleados."cedula" AS empleados_cedula,
     empleados."id_ciudad" AS empleados_id_ciudad,
     empleados."id_cargo" AS empleados_id_cargo,
     empleados."fechanaci" AS empleados_fechanaci,
     empleados."id_sucursal" AS empleados_id_sucursal,
     empleados."caracter" AS empleados_caracter,
     ciudades."id_ciudad" AS ciudades_id_ciudad,
     ciudades."descripcion" AS ciudades_descripcion,
     ciudades."caracter" AS ciudades_caracter,
     cargos."id_cargo" AS cargos_id_cargo,
     cargos."descripcion" AS cargos_descripcion,
     cargos."caracter" AS cargos_caracter
FROM
     "public"."ciudades" ciudades INNER JOIN "public"."empleados" empleados ON ciudades."id_ciudad" = empleados."id_ciudad"
     INNER JOIN "public"."cargos" cargos ON empleados."id_cargo" = cargos."id_cargo"]]>
